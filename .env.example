# Supabase Configuration
# Public URL for your Supabase project
VITE_SUPABASE_URL=https://[project].supabase.co

# Supabase Anonymous/Public Key (safe to use in client-side code)
VITE_SUPABASE_ANON_KEY=

# Optional: Supabase Service Role Key (only use server-side, never expose in client code)
SUPABASE_SERVICE_ROLE_KEY=

# Optional: Direct Database Connection String
DATABASE_URL=postgresql://postgres:[password]@db.[project].supabase.co:5432/postgres

# Optional: Database Password (if needed separately)
DB_PASSWORD=
VITE_OUTLOOK_WORKER_URL=https://sender.dearmp.uk

GEMINI_API_KEY=
EDGE_FUNCTION_URL=https://[project].supabase.co/functions/v1
