  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^8.55.0",
    "supertest": "^7.1.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
